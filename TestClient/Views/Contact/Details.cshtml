@model Contact

@{
    ViewData["Title"] = "Details";
}

<h1>Contact Details</h1>

<div>
    <h4>Contact</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Firstname</dt>
        <dd class="col-sm-10">@Model.Firstname</dd>
        <dt class="col-sm-2">Surname</dt>
        <dd class="col-sm-10">@Model.Surname</dd>
        <dt class="col-sm-2">KnownAs</dt>
        <dd class="col-sm-10">@Model.KnowAs</dd>
        <dt class="col-sm-2">OfficePhone</dt>
        <dd class="col-sm-10">@Model.OfficePhone</dd>
        <dt class="col-sm-2">MobilePhone</dt>
        <dd class="col-sm-10">@Model.MobilePhone</dd>
        <dt class="col-sm-2">StHomePhone</dt>
        <dd class="col-sm-10">@Model.StHomePhone</dd>
        <dt class="col-sm-2">EmailAddress</dt>
        <dd class="col-sm-10">@Model.EmailAddress</dd>
        <dt class="col-sm-2">ManagerName</dt>
        <dd class="col-sm-10">@Model.ManagerName</dd>
        <dt class="col-sm-2">ContactType</dt>
        <dd class="col-sm-10">@Model.ContactType</dd>
        <dt class="col-sm-2">BestContactMethod</dt>
        <dd class="col-sm-10">@Model.BestContactMethod</dd>
        <dt class="col-sm-2">JobRole</dt>
        <dd class="col-sm-10">@Model.JobRole</dd>
        <dt class="col-sm-2">Workbase</dt>
        <dd class="col-sm-10">@Model.Workbase</dd>
        <dt class="col-sm-2">JobTitle</dt>
        <dd class="col-sm-10">@Model.JobTitle</dd>
        <dt class="col-sm-2">IsActive</dt>
        <dd class="col-sm-10">@Model.IsActive</dd>
    </dl>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>
